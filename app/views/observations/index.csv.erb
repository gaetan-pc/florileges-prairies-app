<% headers = [
	'obs_id',
	'site_id',
	'site_code',
	'releve_date',
	'releve_name',
	'releve_structure',
	'releve_fonction',
	'releve_time_start',
	'releve_time_end',
	'releve_hauteur',
	'releve_milieux',
	'releve_user_id',
	'releve_semis',
	'releve_fauche',
	'releve_fauche_periode',
	'releve_fauche_freq',
	'releve_fauche_export',
	'releve_paturage',
	'releve_paturage_pression',
	'releve_paturage_duree',
	'releve_traitement',
	'releve_pression',
	'releve_created_at',
	'releve_updated_at',
	'obs_INPUT_VAL',
	'obs_NOM_VALIDE',
	'obs_CD_NOM',
	'obs_CD_REF',
	'obs_florileges',
	'obs_TAXREF_V',
	'obs_q1',
	'obs_q2',
	'obs_q3',
	'obs_q4',
	'obs_q5',
	'obs_q6',
	'obs_q7',
	'obs_q8',
	'obs_q9',
	'obs_q10',
	'obs_hors_q',
	'obs_created_at',
	'obs_updated_at'] %>
<%= CSV.generate_line(headers, {:row_sep => ''}).html_safe %>
<% @observations.each do |obs| %>
<%= CSV.generate_line([
	obs.id,
	obs.releve.site_id,
	obs.releve.site.code,
	obs.releve.date,
	obs.releve.name,
	obs.releve.structure,
	obs.releve.fonction,
	obs.releve.time_start.to_s.gsub("01/01/2000 ", '') ,
	obs.releve.time_end.to_s.gsub("01/01/2000 ", '') ,
	obs.releve.hauteur,
	obs.releve.milieux.split('|'),
	obs.releve.user_id,
	obs.releve.semis,
	obs.releve.fauche,
	obs.releve.fauche_periode,
	obs.releve.fauche_freq,
	obs.releve.fauche_export,
	obs.releve.paturage,
	obs.releve.paturage_pression,
	obs.releve.paturage_duree,
	obs.releve.traitement,
	obs.releve.pression,
	obs.releve.created_at,
	obs.releve.updated_at,
	obs.LB_NOM,
	obs.NOM_VALIDE,
	obs.CD_NOM,
	obs.CD_REF,
	obs.florileges,
	obs.TAXREF_V,
	obs.q1,
	obs.q2,
	obs.q3,
	obs.q4,
	obs.q5,
	obs.q6,
	obs.q7,
	obs.q8,
	obs.q9,
	obs.q10,
	obs.q0,
	obs.created_at,
	obs.updated_at], {:row_sep => ''}).html_safe %>
<% end %>


