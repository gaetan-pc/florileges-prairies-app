<%= simple_form_for(@observation) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :q0 %>
    <%= f.input :q1 %>
    <%= f.input :q2 %>
    <%= f.input :q3 %>
    <%= f.input :q4 %>
    <%= f.input :q5 %>
    <%= f.input :q6 %>
    <%= f.input :q7 %>
    <%= f.input :q8 %>
    <%= f.input :q9 %>
    <%= f.input :q10 %>
    <%= f.input :p %>
    <%= f.association :espece %>
    <%= f.association :releve %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
